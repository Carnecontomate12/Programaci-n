	String raza;
	Boolean pulgas;
	public Perro(String nombre, int edad, String estado, LocalDate fechaNacimiento) {
		super(nombre, edad, estado, fechaNacimiento);
	}
	@Override
	public String muestra() {
		return null;
	}

	@Override
	public boolean hablar() {
		return false;
	}

}
