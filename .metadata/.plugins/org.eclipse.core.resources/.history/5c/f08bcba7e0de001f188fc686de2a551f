package Orientacionobjetos;

public class Gato {
	String color;
	String raza;
	String sexo;
	int edad;
	float peso;
	
	
	public void maullar () {
		System.out.println("Miauuuu");
		
	}
	public void ronroneo () {
		System.out.println("Miauuuu");
	}
	public void comer() {
		if (raza.equals("gato")) {
			comer();
		}else {
			System.out.println("Te vas a quedar esmallao");
		}
	}
}