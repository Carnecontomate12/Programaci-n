package Boletin3orientacion;

import java.time.LocalDate;

public class Ej3GestionBanco {
	public static void main(String[] args) {
		Ej3Persona titular = new Ej3Persona ("12345678B", "Miguel", "Torres", LocalDate.of(2004, 10, 12));
	
		Ej3CuentaBancaria cuenta = new Ej3CuentaBancaria (0.0, "ES1273487532174", titular);
		
		cuenta.setSaldo(100);
	}
}
